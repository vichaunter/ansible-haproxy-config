# {{ ansible_managed }}
{% for acl in frontend.acls | default([]) %}
  acl {{acl.name}} {{acl.condition}}
  {% if acl.backend is defined %}
  use_backend {{acl.backend}} if {{acl.name}}
  {% endif %}
  {% if acl.http_request is defined %}
  http-request {{acl.http_request}}
  {% endif %}
{% endfor %}